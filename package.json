{
  "name": "taetaetin",
  "version": "0.0.1",
  "description": "လက်မှု နှင့် လက်ဆောင်",
  "productName": "သေးသေးတင်",
  "author": "KWARN KHAM <kwarnkham.moon@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue ./",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "build-ttt": "quasar clean && cp .env.prod .env && npm run build",
    "build-mm": "quasar clean && cp .env.prod.mm .env && npm run build",
    "build": "quasar build && cp .env.local .env && npm run export",
    "zip-macos": "ditto -c -k --sequesterRsrc --keepParent dist/spa spa.zip",
    "zip-windows": "cd dist && zip -r ../spa.zip ./spa && cd ../",
    "prepare-build": "quasar clean && cp .env.prod .env",
    "export": "rm -f spa.zip && if [[ $OSTYPE == msys ]]; then npm run zip-windows; else npm run zip-macos; fi"
  },
  "dependencies": {
    "@quasar/cli": "^1.4.0",
    "@quasar/extras": "^1.15.10",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "dom-to-image": "^2.6.0",
    "dotenv": "^16.0.3",
    "pinia": "^2.0.11",
    "quasar": "^2.11.5",
    "vue": "^3.0.0",
    "vue-i18n": "^9.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.13.14",
    "@quasar/app-webpack": "^3.7.1",
    "ajv": "^8.12.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "eslint-webpack-plugin": "^3.1.1",
    "prettier": "^2.5.1",
    "workbox-webpack-plugin": "^6.5.4"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": ">= 12.22.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
